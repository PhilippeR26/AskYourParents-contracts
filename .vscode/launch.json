{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "TS Project",
            "program": "${workspaceFolder}/src/deployECU.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "sourceMaps": true,
            "smartStep": true,
            "internalConsoleOptions": "openOnSessionStart",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npx hardhat TEST starknet",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceFolder}/node_modules/hardhat/internal/cli/cli.js",
            "args": [
                "test",
                "--verbose",
                "${workspaceFolder}/test/testAdmin.ts"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/test",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npx hardhat ETH localnet",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceFolder}/node_modules/hardhat/internal/cli/cli.js",
            "args": [
                "node",
                "--verbose"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npx hardhat RUN starknet",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceFolder}/node_modules/hardhat/internal/cli/cli.js",
            "args": [
                "run",
                "--verbose",
                "${workspaceFolder}/scripts/5.deployCWallet.ts"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts",
        }
    ]
}